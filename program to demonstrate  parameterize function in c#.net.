using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace CSharp_Shell
{
           public class Program 
    {  
          int x=0;
    void print(int x,int y)
	{
		x=x+y;
	Console.WriteLine(x);
	}
        public static void Main()
     {
	Program p=new Program();
           p.print(20,20);
           Console.ReadLine();
        } 
 
     }
}

